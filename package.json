{
    "name": "notebook",
    "main": "build/index.js",
    "type": "module",
    "scripts": {
        "build:app": "esbuild src/index.ts --outdir=build --bundle --sourcemap --splitting --format=esm --platform=node",
        "build:_ln": "cat package.json | jq -r '{name} + {version, type, typings, main: \"./index.js\"}' > build/package.json && $npm_execpath install",
        "start": "node_modules/@nodegui/qode/binaries/qode run.js",
        "postinstall": "node lib/install-ng.js",
        "build": "cat package.json | jq -r '.scripts | keys_unsorted[] | select(. | startswith(\"build:\"))' | xargs -d \\\\n -I {} $npm_execpath run {}"
    },
    "dependencies": {
        "@j-cake/jcake-utils": "latest",
        "chalk": "latest",
        "lodash": "latest",
        "@nodegui/qode": "latest",
        "$app": "link:build",
        "nodegui": "link:lib/nodegui"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/lodash": "latest",
        "typescript": "latest",
        "esbuild": "latest"
    }
}
